{
  "name": "TestC",
  "version": "1.0.1",
  "summary": "TestC SDK.",
  "description": "TestC SDK for iOS.",
  "homepage": "http://www.yunduoketang.com",
  "license": "MIT",
  "authors": {
    "dxj": "qq@163.com"
  },
  "platforms": {
    "ios": "11.0"
  },
  "source": {
    "git": "https://github.com/dongMan/dxjRouter.git",
    "tag": "1.0.1"
  },
  "requires_arc": true,
  "static_framework": true,
  "swift_versions": "5.0",
  "prefix_header_contents": "",
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) TestC_NAME=TestC TestC_VERSION=1.0.1"
  },
  "user_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "PODTESTC=1",
    "OTHER_SWIFT_FLAGS": "-D PODTESTC"
  },
  "subspecs": [
    {
      "name": "C",
      "source_files": [
        "TestC/TestC/C/*.{h,m,swift}",
        "TestC/TestC/C/**/*.{h,m,swift}"
      ],
      "resources": [
        "TestC/TestC/C/*.{xib}",
        "TestC/TestC/C/**/*.{xib}"
      ],
      "resource_bundles": {
        "TestC": [
          "TestC/TestC/Assets.xcassets"
        ]
      },
      "dependencies": {
        "TestBasis": [

        ]
      },
      "ios": {
        "frameworks": "UIKit"
      }
    }
  ],
  "swift_version": "5.0"
}
